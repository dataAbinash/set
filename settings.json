{
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.explorerKind": "external",
  "terminal.integrated.smoothScrolling": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.ignoreSelection": true,
  "code-runner.runInTerminal": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt.exe && $dir$fileNameWithoutExt.exe",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt.exe && $dir$fileNameWithoutExt.exe",
    "python": "py -u",
    "typescript": "deno run",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded"
  },
  "code-runner.languageIdToFileExtensionMap": {
    "bat": ".bat",
    "powershell": ".ps1",
    "typescript": ".ts"
  },
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.suggest.preview": true,
  "editor.suggest.showStatusBar": true,
  "editor.suggest.localityBonus": true,
  "editor.wordBasedSuggestionsMode": "allDocuments",
  "files.autoGuessEncoding": true,
  "window.dialogStyle": "custom",
  "explorer.incrementalNaming": "smart",
  "explorer.confirmDelete": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "editor.smoothScrolling": true,
  "liveServer.settings.useLocalIp": true,
  "workbench.list.smoothScrolling": true,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "workbench.colorCustomizations": {
    "[One Dark Pro Darker]": {
      "sideBar.border": "#141a24",
      "activityBar.border": "#141a24",
      "titleBar.border": "#141a24",
      "statusBar.border": "#141a24",
      "editor.background": "#0d1117",
      "sideBar.background": "#0d1117",
      "statusBar.background": "#161b22",
      // "sideBar.dropBackground": "#111",
      // "activityBar.activeBackground": "#181f2b",
      "activityBar.activeBackground": "#1a222c",
      "activityBar.background": "#0d1117",
      "activityBar.activeBorder": "#8866FF",
      "editorGroupHeader.tabsBackground": "#0d1117",
      // "editorGroupHeader.border": "#000000",
      // "editorGroup.border": "#141a24",
      "tab.activeBackground": "#0d1117",
      "tab.inactiveBackground": "#161b22",
      // "button.background": "#8866FF",
      "titleBar.activeBackground": "#0d1117",
      "button.foreground": "#fff",
      "textLink.foreground": "#b547ff",
      // "terminal.background": "#161b22",
      "terminal.background": "#090b0f",
      // "tab.border": "#141a24"
      "titleBar.inactiveBackground": "#0d1117",
      // "panel.border": "#8866FF"
      // "panelTitle.activeBorder": "#8866FF",
      // "menu.selectionBackground": "#8866FF",
      // "menu.selectionForeground": "#fff",
      "menubar.selectionBorder": "#b547ff"
    }
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true,
  "editor.rename.enablePreview": false,

  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
  "explorer.confirmDragAndDrop": false,
  "editor.linkedEditing": true,
  // Tab autocompletion
  "editor.tabCompletion": "on",
  "emmet.triggerExpansionOnTab": true,
  "editor.quickSuggestions": {
    "comments": "on",
    "strings": "on"
  },
  "editor.stickyScroll.enabled": true,
  "codesnap.backgroundColor": "transparent",
  "codesnap.shutterAction": "copy",
  "editor.accessibilitySupport": "off",
  "auto-rename-tag.activationOnLanguage": ["*"],
  "git.enableSmartCommit": true,
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "debug.onTaskErrors": "debugAnyway",
  "editor.inlineSuggest.enabled": true,
  "cSpell.languageSettings": [],
  "git.confirmSync": false,
  "editor.tabSize": 3,
  "github.copilot.enable": {
    "*": true,
    "plaintext": true,
    "markdown": true,
    "scminput": false,
    "c": true,
    "javascript": true,
    "typescript": true,
    "cpp": true
  },
  "extensions.ignoreRecommendations": true,
  "workbench.iconTheme": "material-icon-theme",
  "cSpell.userWords": [
    "Abinash",
    "ACCUWEATHER",
    "airtel",
    "Ananta",
    "Antu",
    "bsnl",
    "codeabinash",
    "cout",
    "fibo",
    "inorder",
    "Karmakar",
    "Leetcode",
    "millis",
    "minifier",
    "nums",
    "reduxjs",
    "Subarray",
    "tailwindcss",
    "Topup",
    "Trapp",
    "webp"
  ],
  "markdown.validate.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.rulers": [
    {
      "color": "#00ff005e",
      "column": 120
    }
  ],
  "editor.selectionHighlight": false,
  "git.ignoreRebaseWarning": true,
  "notebook.markup.fontSize": 13,
  "oneDarkPro.vivid": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,

  "hediet.vscode-drawio.resizeImages": null,
  "editor.cursorSmoothCaretAnimation": "on",
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "'Cascadia Code', 'Courier New', monospace",
  "codesnap.boxShadow": "rgba(0, 0, 0, 0) 0px 20px 68px",
  "codesnap.containerPadding": "2em",
  "diffEditor.experimental.useVersion2": true,
  "workbench.experimental.share.enabled": true,
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "tabnine.experimentalAutoImports": true,
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "HTML"
  },
  "svg.preview.mode": "img",
  "terminal.integrated.cursorBlinking": true,
  "editor.cursorBlinking": "smooth",
  "editor.guides.indentation": false,
  "git.defaultCloneDirectory": "D:/Programming/git",
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.startupEditor": "welcomePageInEmptyWorkbench",
  "workbench.colorTheme": "One Dark Pro Darker",
  "editor.formatOnSave": true,
  "update.mode": "start",
  "workbench.editor.showTabs": "multiple",
  "editor.fontSize": 18,
  "C_Cpp.intelliSenseUpdateDelay": 500,
  "C_Cpp.vcFormat.newLine.beforeElse": false,
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyFunction": true,
  "C_Cpp.vcFormat.newLine.closeBraceSameLine.emptyType": true,

  // Disable New line after if statement without braces
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.block": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.lambda": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.namespace": "sameLine",
  "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "sameLine",

  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 3 }",
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  "C_Cpp.default.intelliSenseMode": "windows-gcc-x64",
  "C_Cpp.debugShortcut": false,
  "C_Cpp.default.compilerPath": "D:\\Apps\\MinGW\\bin\\gcc.exe",
  "settingsSync.ignoredSettings": [
    "-C_Cpp.default.compilerPath",
    "-C_Cpp.default.compilerArgs",
    "-C_Cpp.default.compileCommands",
    "-C_Cpp.default.browse.path",
    "-C_Cpp.default.browse.databaseFilename"
  ],
  "terminal.external.windowsExec": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "grunt.autoDetect": "on"
}
